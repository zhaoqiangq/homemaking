/**index.wxss**/
page{ height: 100%;} 
.seach{
  width: 100%;
  height: 80rpx;
  background: #4FA4F8;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  position: fixed;
  top:0;left: 0;right: 0;
  margin: auto;
}
.seach .seachInner{
  width: 100%;
  height: 100%;
  background: #fff;
  border-radius: 50rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.seach .seachInner image{
  width: 35rpx;
  float: left;
  margin-top: 15rpx;
}
.seach input{
  font-size: 25rpx;
  float: left;
  height: 100%;
  width: 80%;
  margin-left: 15rpx;
}

.nav{
  position: fixed;
  top:80rpx;left: 0;bottom: 0;
  width: 160rpx;
  margin: auto;
  background: #F0F0F0;
}
.nav view{
  font-size: 28rpx;
  padding: 20rpx;
  color: #666;
  font-weight: 500;
  position: relative;
}
.nav view::after{
  content: "";
  width: 5rpx;
  height: 100%;
  background: #4FA4F8;
  position: absolute;
  left: 0;top:0;bottom: 0;
  margin: auto;
  display: none;
}
.nav view.active{
  background: #fff;
  color: #4FA4F8;
}
.nav view.active::after{
  display: block;
}
.content{
  padding-bottom: 20rpx; 
  box-sizing: border-box; 
  height: 100%;
  padding-left: 160rpx;
  padding-right: 20rpx;
  padding-top: 90rpx;
} 
.inner{
  margin-bottom: 40rpx;
}
.inner::after{
  content: "";
  clear: both;
  display: block;
}
.h2{
  font-size: 32rpx;
  float: none;
  margin-left: 3%;
}
.item{
  width: 47%;
  float: left;
  font-size: 28rpx;
  margin-left: 3%;
  background: #F0F0F0;
  margin-top: 20rpx;
  text-align: center;
  padding-top: 20rpx;
  color: #585858;
  padding-bottom: 20rpx;
}
.item image{
  width: 100rpx;
}
.price{
  margin-top: 10rpx;
  color: #cd2e2e;
  font-size: 25rpx;
}
.images{
  width: 100%;
}